<template id="template.css">
    <style class="customcss">
            {{#wcolor}}
            .widget-preview .widget,
            .widgets-container .widget,
            .store > .detail > .preview-container > .preview > .widget {
                background: {{wcolor}};
            }
            {{/wcolor}}

            {{#hcolor}}
            .toolbar {
                background: {{hcolor}};
            }
            {{/hcolor}}

            {{^animation}}
            .unloaded .widgets-container .widget {
                -webkit-animation: none!important;
            }
            {{/animation}}

            {{{custom}}}
    </style>
</template>
